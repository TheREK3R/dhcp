# File managed by Chef

class "<%= @name %>" {
  match <%= @match %>;
  <% unless @vendor_option_space.nil? || @vendor_option_space.empty? -%>
  vendor-option-space <%= @vendor_option_space %>;
  <% end -%>
  <% unless @options.nil? || @options.empty? -%>
  <% @options.each do |value| -%>
  option "<%= @name %>" <%= value %>;
  <% end -%>
}
<% unless @subclasses.nil? || @subclasses.empty? -%>
<% @subclasses.each do |value| -%>
subclass "<%= @name %>" <%= value %>;
<% end -%>
<% end -%>
